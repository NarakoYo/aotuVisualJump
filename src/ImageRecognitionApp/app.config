<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- 日志配置 -->
    <add key="Log.MaxFileSizeKB" value="1024"/>
    <add key="Log.MaxFilesPerDay" value="10"/>
    <add key="Log.MinLevel" value="Info"/>
    <add key="Log.FileNameFormat" value="yyyy-MM-dd-HH"/>
  </appSettings>
  <system.web>
    <globalization
      requestEncoding="utf-8"
      responseEncoding="utf-8"
      fileEncoding="utf-8"
      culture="zh-CN"
      uiCulture="zh-CN"
    />
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="bin;libs"/>
    </assemblyBinding>
  </runtime>
</configuration>